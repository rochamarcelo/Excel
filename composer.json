{
    "name": "rocha-marcelo/excel",
    "description": "Excel plugin for CakePHP to handle import and export",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "box/spout": "^3.3",
        "cakephp/cakephp": "4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "RoMa\\Excel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RoMa\\Excel\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
